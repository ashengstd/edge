{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": ["@types/bun"],
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "moduleDetection": "force"
  }
}
